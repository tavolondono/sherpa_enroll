<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="?attr/layoutFragment"
    >

    <include layout="@layout/steps_status"
        android:id="@+id/stepSatus"
        style="?attr/layoutStepStatus"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        />
    
	<ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fillViewport="true"

        android:layout_alignParentTop="true"
        android:layout_above="@+id/layoutButtons"
        android:layout_below="@+id/stepSatus"
		>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/layoutBody"
            android:gravity="center"
            style="?attr/layoutBody"
            >
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/icon"
                style="?attr/iconLargeVerificationFaceFailed"
                android:layout_centerHorizontal="true"
                />

            <TextView
                android:layout_below="@+id/icon"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/label_registration_authentication_fail_title"
                android:id="@+id/label_title"
                style="?attr/titleText"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/label_registration_authentication_fail_description"
                android:id="@+id/label_description"
                android:layout_below="@+id/label_title"
                style="?attr/contentText"
                />

            </RelativeLayout>
	</ScrollView>

    <RelativeLayout
        android:id="@+id/layoutButtons"
        style="?attr/layoutDoubleButton"
        >
        <Button
            android:layout_width="match_parent"
            android:text="@string/button_try_again"
            android:id="@+id/buttonNegative"
            style="?attr/negativeButton"
            android:layout_alignParentBottom="true"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="@dimen/margin_xlarge"
            android:text="@string/label_or"
            android:id="@+id/label_or"
            android:layout_above="@+id/buttonNegative"
            android:layout_centerHorizontal="true"
            style="?attr/contentText"
            />

        <Button
            android:layout_width="match_parent"
            android:text="@string/button_see_tutorial"
            android:id="@+id/buttonNeutral"
            android:layout_above="@+id/label_or"
            style="?attr/neutralButton"
            />
    </RelativeLayout>


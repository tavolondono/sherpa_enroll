<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="?attr/layoutFragment">

    <include layout="@layout/steps_status"
        android:id="@+id/stepSatus"
        style="?attr/layoutStepStatus"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fillViewport="true"
        android:layout_above="@+id/layoutButtons"
        android:layout_below="@+id/stepSatus"
        >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/layoutBody"
        android:gravity="center"
        style="?attr/layoutBody"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/label_capture_face_welcome_description"
            android:id="@+id/label_description"
            style="?attr/contentText"
            />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/layoutContent"
            android:orientation="vertical"
            android:layout_below="@+id/label_description"
            android:layout_alignLeft="@+id/label_description"
            >


        </LinearLayout>

    </RelativeLayout>
    </ScrollView>

    <RelativeLayout
        android:id="@+id/layoutButtons"
        style="?attr/layoutDoubleButton"
        >
        <Button
            android:layout_width="match_parent"
            android:text="@string/button_start_face_registration"
            android:id="@+id/buttonPositive"
            style="?attr/positiveButton"
            android:layout_alignParentBottom="true"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="@dimen/margin_xlarge"
            android:text="@string/label_or"
            android:id="@+id/label_or"
            android:layout_above="@+id/buttonPositive"
            android:layout_centerHorizontal="true"
            style="?attr/contentText"
            />

        <Button
            android:layout_width="match_parent"
            android:text="@string/button_see_tutorial"
            android:id="@+id/buttonNeutral"
            android:layout_above="@+id/label_or"
            style="?attr/neutralButton"
            />
    </RelativeLayout>

